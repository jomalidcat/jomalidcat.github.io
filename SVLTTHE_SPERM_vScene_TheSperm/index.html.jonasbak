    visibility: hidden;
    display: flex;
    top:0px;
    left: 0px;
    height: 32px;
    width: 32px;
    -webkit-user-select: none; /* Safari */        
    -moz-user-select: none; /* Firefox */
    -ms-user-select: none; /* IE10+/Edge */
    user-select: none; /* Standard */
    pointer-events: none;
    z-index: 3;
  }

  .inking-cursor {
    position: relative;
    height: 24px;
    width: 24px;
    margin: auto;
    left:-50%;
    top:-50%;
    border-radius: 50%;
    border: 1px white solid;
    background-color: transparent;
    -webkit-user-select: none; /* Safari */        
    -moz-user-select: none; /* Firefox */
    -ms-user-select: none; /* IE10+/Edge */
    user-select: none; /* Standard */
    pointer-events: none;
  }

    </style>
